package baek_math1;

import java.util.Scanner;

public class B_2292 {

	public static void main(String[] args) {
		int num;
		int count=1;
		int range=1;
		int sum=0;
		Scanner sc=new Scanner(System.in);
		num=sc.nextInt();
		
		while(true) {
			if(num<=range) {
				System.out.println(count);
				break;
			}
			count++;
			sum+=6;
			range+=sum;
		}
	}

}
