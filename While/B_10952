package baek_while;

import java.util.Scanner;

public class B_10952 {

	public static void main(String[] args) {
		int a=2,b=2;
		int sum=0;
		String result="";
		Scanner sc=new Scanner(System.in);
		while(true) {
			a=sc.nextInt();
			b=sc.nextInt();
			if(a==0&&b==0)
				break;
			sum=a+b;
			result=result+Integer.toString(sum)+"\n";
		}
		System.out.println(result);
		sc.close();
	}

}
